{
  "watch": ["src"],
  "ext": "ts,json",
  "ignore": ["src/**/*.test.ts", "src/**/*.spec.ts", "node_modules"],
  "exec": "node -r ./tsconfig-paths-bootstrap.js -r ts-node/register src/server.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "signal": "SIGTERM",
  "verbose": true
}
